{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|jsdocs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "destination": "./jsdocs/",
    "template": "./custom-jsdoc-template",
    "readme": "./jsdoc-readme/readme.md"
  }
}
